{
  "name": "@app/graphql",
  "private": true,
  "version": "0.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "yarn codegen && tsc -b",
    "watch": "yarn codegen --watch",
    "codegen": "graphql-codegen --config codegen.yml",
    "depcheck": "depcheck --ignores=\"@graphql-codegen/*,@urql/*,urql,graphql-tag,graphql-codegen-persisted-query-ids,tslib\""
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.1.0",
    "@graphql-codegen/cli": "^2.2.0",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/typescript": "^2.2.2",
    "@graphql-codegen/typescript-operations": "^2.1.6",
    "@graphql-codegen/typescript-urql": "^3.2.0",
    "@graphql-codegen/typescript-urql-graphcache": "^2.1.4",
    "@graphql-codegen/urql-introspection": "^2.1.0",
    "@urql/introspection": "^0.3.0",
    "graphql-codegen-persisted-query-ids": "^0.1.2"
  }
}
